<!DOCTYPE html>
<html>
  <head>
	<title><%= @title %></title>
	<%= csrf_meta_tag %>
	<%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults, "jquery.cycle.lite.min.js" %>
<script type="text/javascript"> 
$(document).ready(function() {
  $('#slideshow1').cycle({
    fx: 'fade',	   // transition type
    timeout: 5000  // time (ms) between beginning of each transition
  });
});

</script>
  </head>
  <body>
  	<div class="container">
  		<div>
      <%= render 'layouts/header' %>
      </div>
      <section class="round">
          <%= yield %>
      </section>	
      <div>
        <%= render 'layouts/footer' %>
      </div>
  	</div>
  </body>
</html>